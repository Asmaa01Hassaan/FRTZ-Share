<odoo>
    <template id="report_delivery_document_inherit" inherit_id="stock.report_delivery_document">


        <xpath expr="//t[@t-set='address']" position="before">
            <div t-if="o.picking_type_id" class="row"
                 t-attf-style="direction: {{ 'rtl' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'ltr' }}; text-align: {{ 'center' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'left' }};">
                <div class="col-8">
                    <span t-field="o.picking_type_id"/>
                </div>
            </div>
        </xpath>

        <xpath expr="//t[@t-set='address']" position="after">
            <h1 style="padding:30px 0 15px; 0"></h1>
            <div class="container-fluid mb-4"
                 t-attf-style="direction: {{ 'rtl' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'ltr' }}; text-align: {{ 'right' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'left' }};">

                <div class="row">
                    <div class="col-6">
                        <strong>
                            <t t-if="not o.partner_id or o.partner_id.lang == 'ar_001'">تاريخ التوصيل الفعلي:</t>
                            <t t-else="">Shipping Date:</t>
                        </strong>
                        <span t-field="o.date_done" t-options='{"widget": "date"}' class="ms-2"/>
                    </div>
                    <div class="col-6">
                        <strong>
                            <t t-if="not o.partner_id or o.partner_id.lang == 'ar_001'">تاريخ التوصيل المجدول:</t>
                            <t t-else="">Scheduled Date:</t>
                        </strong>
                        <span t-field="o.scheduled_date" t-options='{"widget": "date"}' class="ms-2"/>
                    </div>
                </div>

                <div class="row mt-2">
                    <div class="col-6" t-if="o.origin">
                        <strong>
                            <t t-if="not o.partner_id or o.partner_id.lang == 'ar_001'">أمر الطلب:</t>
                            <t t-else="">Order:</t>
                        </strong>
                        <span t-field="o.origin" class="ms-2"/>
                    </div>
                    <div class="col-6" t-if="o.location_id">
                        <strong>
                            <t t-if="not o.partner_id or o.partner_id.lang == 'ar_001'">الموقع:</t>
                            <t t-else="">Location:</t>
                        </strong>
                        <span t-field="o.location_id.display_name" class="ms-2"/>
                    </div>
                </div>

                <div class="row mt-2" t-if="o.partner_id">
                    <div class="col-12">
                        <strong>
                            <t t-if="not o.partner_id or o.partner_id.lang == 'ar_001'">جهة الاتصال:</t>
                            <t t-else="">Contact:</t>
                        </strong>
                        <span t-field="o.partner_id" class="ms-2"/>
                    </div>
                </div>
            </div>

        </xpath>

        <xpath expr="//div[@id='informations']" position="attributes">
            <attribute name="style">display: none !important;</attribute>
            <attribute name="class">d-none</attribute>
        </xpath>

        <xpath expr="//h2" position="replace">
            <h2 style="text-align:center;">
                <span t-field="o.name">WH/OUT/0001</span>
            </h2>
        </xpath>

        <xpath expr="//div[@name='div_outgoing_address']" position="attributes">
            <attribute name="style">display: none !important;</attribute>
            <attribute name="class">d-none</attribute>
        </xpath>

        <xpath expr="//table[@name='stock_move_table']" position="attributes">
            <attribute name="t-att-dir">'rtl' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'ltr'
            </attribute>
            <attribute name="t-attf-style">width: 100% !important; border-collapse: collapse !important; border: 1px
                solid black !important; {{ 'direction: rtl; text-align: center;' if not o.partner_id or
                o.partner_id.lang
                == 'ar_001' else 'direction: ltr; text-align: center;' }}
            </attribute>
        </xpath>

        <xpath expr="//table[@name='stock_move_table']/thead/tr/th" position="attributes">
            <attribute name="style">
                border: 1px solid black !important;
                text-align: center !important;
                padding: 10px;
                background-color: transparent !important;
            </attribute>
        </xpath>

        <xpath expr="//table[@name='stock_move_table']/thead/tr/th[@name='th_sm_product']" position="attributes">
            <attribute name="style">
                border: 1px solid black !important;
                text-align: center !important;
                vertical-align: middle !important;
                padding: 8px;
            </attribute>
        </xpath>

        <xpath expr="//table[@name='stock_move_table']/tbody/tr/td" position="attributes">
            <attribute name="style">
                border: 1px solid black !important;
                text-align: center !important;
                vertical-align: middle !important;
                padding: 8px;
            </attribute>
        </xpath>

        <xpath expr="//table[@name='stock_move_line_table']" position="attributes">
            <attribute name="t-att-dir">'rtl' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'ltr'
            </attribute>
            <attribute name="t-attf-style">width: 100% !important; border-collapse: collapse !important; border: 1px
                solid black !important; {{ 'direction: rtl; text-align: right;' if not o.partner_id or o.partner_id.lang
                == 'ar_001' else 'direction: ltr; text-align: left;' }}
            </attribute>
        </xpath>

        <xpath expr="//table[@name='stock_move_line_table']/thead/tr/th" position="attributes">
            <attribute name="style">border: 1px solid black !important; padding: 10px; background-color: transparent
                !important;
            </attribute>
        </xpath>
        <xpath expr="//table[@name='stock_move_line_table']/tbody" position="attributes">
            <attribute name="style">border: 1px solid black !important; padding: 8px; vertical-align: middle;
            </attribute>
        </xpath>


        <xpath expr="//table[@name='stock_backorder_table']" position="attributes">
            <attribute name="t-att-dir">'rtl' if not o.partner_id or o.partner_id.lang == 'ar_001' else 'ltr'
            </attribute>
            <attribute name="t-attf-style">width: 100% !important; border-collapse: collapse !important; border: 1px
                solid black !important; {{ 'direction: rtl; text-align: right;' if not o.partner_id or o.partner_id.lang
                == 'ar_001' else 'direction: ltr; text-align: left;' }}
            </attribute>
        </xpath>

        <xpath expr="//table[@name='stock_backorder_table']/thead/tr/th" position="attributes">
            <attribute name="style">border: 1px solid black !important; padding: 10px; background-color: transparent
                !important;
            </attribute>
        </xpath>
        <xpath expr="//table[@name='stock_backorder_table']/tbody" position="attributes">
            <attribute name="style">border: 1px solid black !important; padding: 8px; vertical-align: middle;
            </attribute>
        </xpath>

        <xpath expr="//t[@t-set='information_block']" position="attributes">
            <attribute name="t-value">null</attribute>
        </xpath>
        <xpath expr="//t[@t-set='address']" position="replace">
            <t t-set="address" t-value="False"/>
        </xpath>


    </template>


